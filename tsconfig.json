{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "lib": ["DOM", "DOM.Iterable", "ES2020"],
    "allowJs": true,
    "checkJs": false,
    "noEmit": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "types": ["chrome"],
    "moduleResolution": "node"
  },
  "include": ["js/**/*.js", "types/**/*.d.ts"]
}
